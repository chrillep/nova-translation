{
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "globals": {
    "Nova": true,
    "moment": true,
    "axios": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "vue/html-indent": ["error", 4],
    "semi": ["error", "never"]
  }
}
