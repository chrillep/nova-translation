{
  "name": "njimx/nova-translation",
  "description": "A Laravel Nova tool.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "joedixon/laravel-translation": "^1.0.0"
  },
  "require-dev": {
    "inertiajs/inertia-laravel": "^0.5.4",
    "laravel/nova": "^4.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Joedixon\\NovaTranslation\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Joedixon\\NovaTranslation\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
